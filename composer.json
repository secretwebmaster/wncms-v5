{
    "name": "secretwebmaster\/wncms",
    "type": "project",
    "description": "Wncms is one of the best CMS in the world.",
    "keywords": [
        "wncms",
        "WNCMS"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "laravel\/framework": "^11.9",
        "laravel\/tinker": "^2.9",
        "secretwebmaster\/wncms-core": "^0.3.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app\/",
            "Database\\Factories\\": "database\/factories\/",
            "Database\\Seeders\\": "database\/seeders\/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests\/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan vendor:publish --tag=wncms-system-config --ansi",
            "@php artisan vendor:publish --tag=wncms-theme-config --ansi",
            "@php artisan vendor:publish --tag=wncms-core-assets --ansi",
            "@php artisan vendor:publish --tag=wncms-theme-assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database\/database.sqlite') || touch('database\/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "mcamara\/laravel-localization"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp\/pest-plugin": true,
            "php-http\/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}